{
  "dashboard": {
    "id": null,
    "title": "KingFisher Service Dashboard v1.1.0",
    "tags": ["kingfisher", "trading-ai", "automation"],
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "panels": [
      {
        "id": 1,
        "title": "Service Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"zmart_services\", service=\"zmart-kingfisher\"}",
            "legendFormat": "Service Up"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP", 
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{service=\"zmart-kingfisher\"}[5m]))",
            "legendFormat": "Requests/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Analysis Latency (P95)",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(kingfisher_analysis_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "P95 Latency"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 15},
                {"color": "red", "value": 30}
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Active Processes",
        "type": "stat",
        "targets": [
          {
            "expr": "kingfisher_active_processes",
            "legendFormat": "Active"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 10,
        "title": "Image Processing Pipeline",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(kingfisher_images_downloaded_total[5m])",
            "legendFormat": "Downloads/sec"
          },
          {
            "expr": "rate(kingfisher_images_deduplicated_total[5m])",
            "legendFormat": "Duplicates/sec"
          },
          {
            "expr": "rate(kingfisher_reports_generated_total[5m])",
            "legendFormat": "Reports/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        }
      },
      {
        "id": 11,
        "title": "Pipeline Failures",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(kingfisher_pipeline_failures_total[5m])",
            "legendFormat": "Failures/sec by {{step}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        }
      },
      {
        "id": 20,
        "title": "Security Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(kingfisher_security_violations_total[5m])",
            "legendFormat": "Violations/sec by {{type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        },
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 12
        }
      },
      {
        "id": 21,
        "title": "Outbox Event Status",
        "type": "timeseries",
        "targets": [
          {
            "expr": "kingfisher_outbox_events_total",
            "legendFormat": "{{status}} Events"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 8,
          "y": 12
        }
      },
      {
        "id": 22,
        "title": "Plugin Execution Times",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(kingfisher_plugin_execution_duration_seconds_bucket[5m])) by (le, plugin))",
            "legendFormat": "P95 {{plugin}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 16,
          "y": 12
        }
      },
      {
        "id": 30,
        "title": "OpenAI Token Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(kingfisher_openai_tokens_total[5m])",
            "legendFormat": "Tokens/sec ({{model}})"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 18
        }
      },
      {
        "id": 31,
        "title": "HTTP Response Status",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(http_requests_total{service=\"zmart-kingfisher\"}[5m])",
            "legendFormat": "{{status}} responses/sec"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 18
        }
      },
      {
        "id": 40,
        "title": "Recent Alerts",
        "type": "table",
        "targets": [
          {
            "expr": "ALERTS{service=\"kingfisher\"}",
            "format": "table",
            "legendFormat": ""
          }
        ],
        "transformations": [
          {
            "id": "filterFieldsByName",
            "options": {
              "include": {
                "names": ["alertname", "severity", "summary", "Time"]
              }
            }
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(up{job=\"zmart_services\", service=\"zmart-kingfisher\"}, instance)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "prometheus",
          "expr": "changes(up{service=\"zmart-kingfisher\"}[5m]) > 0",
          "titleFormat": "KingFisher Deployment",
          "textFormat": "Service restarted"
        }
      ]
    },
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}