{
  "version": 2,
  "builds": [
    {
      "src": "ZmartyUserApp/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/ZmartyUserApp/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/ZmartyUserApp/index.html"
    }
  ]
}