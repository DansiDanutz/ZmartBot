[MESSAGES CONTROL]
# Disable specific warnings for expected environment issues
disable=
    import-error,           # Disable import errors for optional dependencies
    no-member,             # Disable member access errors for optional objects
    unused-import,         # Allow unused imports in test files
    
[TYPECHECK]
# Ignore missing imports for these modules (expected in development)
ignored-modules=
    selenium,
    selenium.webdriver,
    selenium.webdriver.chrome,
    selenium.webdriver.chrome.options,
    selenium.webdriver.chrome.service,
    selenium.webdriver.common,
    selenium.webdriver.common.by,
    selenium.webdriver.support,
    selenium.webdriver.support.ui,
    selenium.common,
    selenium.common.exceptions,
    src.extractors,
    src.extractors.crypto_risk_extractor,
    src.extractors.screener_extractor,
    src.database,
    src.database.cryptoverse_database

# Ignore missing members for these classes
ignored-classes=
    webdriver,
    WebDriver,
    Chrome,
    Options,
    Service,
    By,
    WebDriverWait,
    EC

[BASIC]
# Good variable names for test files
good-names=
    i,j,k,ex,Run,_,
    app,db,api,
    driver,element,
    extractor,generator,
    request,response