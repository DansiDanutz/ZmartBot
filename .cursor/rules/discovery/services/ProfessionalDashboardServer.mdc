const handleLogoError = (e) => {
  console.error('‚ùå Logo failed to load:', e.target.src)
  // Progressive fallback chain
  if (e.target.src.includes('Zmart-Logo-New.jpg')) {
    e.target.src = '/logoZmart.png'
